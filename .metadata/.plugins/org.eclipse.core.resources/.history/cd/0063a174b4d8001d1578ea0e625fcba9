package com.tools.payment.dto;

public class TransacaoDTO {

    private String cartao;
    private String id;
    private DescricaoDTO descricao;
    private String tipo;
    private Integer parcelas;

    public TransacaoDTO() {}

    public TransacaoDTO(String cartao, String id, DescricaoDTO descricao, String tipo, Integer parcelas) {
        this.cartao = cartao;
        this.id = id;
        this.descricao = descricao;
        this.tipo = tipo;
        this.parcelas = parcelas;
    }

	public String getCartao() {
		return cartao;
	}

	public void setCartao(String cartao) {
		this.cartao = cartao;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public DescricaoDTO getDescricao() {
		return descricao;
	}

	public void setDescricao(DescricaoDTO descricao) {
		this.descricao = descricao;
	}

	public String getTipo() {
		return tipo;
	}

	public void setTipo(String tipo) {
		this.tipo = tipo;
	}

	public Integer getParcelas() {
		return parcelas;
	}

	public void setParcelas(Integer parcelas) {
		this.parcelas = parcelas;
	}

}